{
    "name": "patoui/laravel-scout-testing-driver",
    "description": "In memory driver for Laravel Scout to make testing easy",
    "keywords": [
        "testing",
        "search",
        "scout",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Patrique Ouimet",
            "email": "patrique.ouimet@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/scout": "*",
        "illuminate/bus": "~5.4",
        "illuminate/contracts": "~5.4",
        "illuminate/database": "~5.4",
        "illuminate/pagination": "~5.4",
        "illuminate/queue": "~5.4",
        "illuminate/support": "~5.4"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "PatOui\\Scout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
